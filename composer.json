{
    "name": "mmucklo/queue-bundle",
    "description": "Message Queue Manager for symfony2/symfony3",
    "keywords": ["queue", "Message queue"],
    "type": "symfony-bundle",
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dtee/grid.git"
	}   
    ],
    "authors": [
        {
            "name": "David Tee"
        },
    	{
            "name": "Matthew J. Mucklo",
            "email": "mmucklo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
	"mmucklo/grid-bundle": ">=0.9"
    },
    "require-dev": {
        "pda/pheanstalk": "dev-master",
        "videlalvaro/php-amqplib": "dev-master",
        "doctrine/mongodb": "dev-master",
        "doctrine/mongodb-odm": "*"
    },
    "autoload": {
        "psr-0": { "Dtc\\QueueBundle": "" }
    },
    "target-dir": "Dtc/QueueBundle",
    "minimum-stability": "dev",
    "prefer-stable": true
}
